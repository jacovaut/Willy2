<?xml version="1.0"?>
<robot xmlns:xacro="http://ww.ros.org/wiki/xacro" >

    <xacro:include filename="inertial_macros.xacro" />

    <material name="white">
        <color rgba="1 1 1 1" />
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1" />
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1" />
    </material>

    <material name="black">
        <color rgba="0 0 0 1" />
    </material>

    <!-- BASE LINK -->
    <link name="base_link">

    </link>


    <!-- CHASSIS LINK -->
    <joint name="chassis_joint" type="fixed">
        <parent link="base_link" />
        <child link="chassis" />
        <origin xyz="0 0 0.075" />
    </joint>

    <link name="chassis">
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 0.15"/>
            </geometry>
            <material name="white" />
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 0.15"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass ="0.5" x=" 0.3" y="0.3" z="0.15">
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_box>
    </link>

    <gazebo reference="chassis">
        <material>Gazebo/White</material>
    </gazebo>

    <!-- fr WHEEL LINK -->
    <joint name="fr_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="fr_wheel"/>
        <origin xyz="0.15 -0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="fr_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="fr_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>1 -1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- fl WHEEL LINK -->
    <joint name="fl_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="fl_wheel"/>
        <origin xyz="0.15 0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="fl_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="fl_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>1 1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- mr WHEEL LINK -->
    <joint name="mr_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="mr_wheel"/>
        <origin xyz="0 -0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="mr_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="mr_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>1 -1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- ml WHEEL LINK -->
    <joint name="ml_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="ml_wheel"/>
        <origin xyz="0 0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="ml_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="ml_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>1 1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- rr WHEEL LINK -->
    <joint name="rr_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="rr_wheel"/>
        <origin xyz="-0.15 -0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="rr_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="rr_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>-1 -1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- rl WHEEL LINK -->
    <joint name="rl_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="rl_wheel"/>
        <origin xyz="-0.15 0.175 0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="rl_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.04" radius="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="rl_wheel">
        <mu1>0.1</mu1>              <!-- lateral friction (low) -->
        <mu2>100.0</mu2>            <!-- rolling direction friction (high) -->
        <fdir1>-1 1 0</fdir1>       <!-- direction of rollers -->
        <material>Gazebo/Blue</material>
    </gazebo>
</robot>